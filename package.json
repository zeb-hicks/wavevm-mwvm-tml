{
  "name": "wave2-assembly",
  "displayName": "Wave2 Assembly",
  "description": "Assembly Language Support for Meisaka's Wave2 Instruction Set",
  "author": "Nimphious",
  "publisher": "Nimphious",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeb-hicks/wavevm-wave2-asm-tml"
  },
  "version": "0.1.6",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "w2s",
      "aliases": ["Wave2 Assembly", "w2s"],
      "extensions": ["w2s"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "w2s",
      "scopeName": "source.w2s",
      "path": "./syntaxes/w2s.tmLanguage.json"
    }]
  }
